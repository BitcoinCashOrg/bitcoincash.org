    "b40dafc1e3655d2e5cbac924ac784083": {
      "translations": [
        {
          "translation": "Splitting the bill? Send money to your friend's wallet or have them send it to yours instead. Payments are fast and go directly between the two of you—no banks needed."
        }
      ]
    },
    "a3598cd85f5fe560d14f10ecc7cfe1b5": {
